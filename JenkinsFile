pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat './gradlew build'
            }
        }
        stage('Test') {
        steps{
        echo 'TESTING STAGE'
        }
        }
    }
    post {
            always {
                emailext body: 'Body of Test', subject: 'Test', to: 'pallab.netapp.test@gmail.com'
            }
        }

}
